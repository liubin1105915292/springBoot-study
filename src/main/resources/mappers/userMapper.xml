<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.luki.dao.UserDao">
    <select id="quryUserByName" parameterType="string" resultType="user">
        SELECT USER_ID,USER_NAME,PASSWORD FROM luki_user WHERE USER_NAME=#{userName}
    </select>

    <insert id="addUser" parameterType="user">
    insert into luki_user values(sq_user_id.nextval,#{userName},#{password})
    </insert>
</mapper>